// Rod cutting
p := [1, 5, 8, 9, 10, 17, 17, 20, 24, 30];

func max (a, b int) int { return a > b ? a : b ;}

func cutRod (n int) int {
  if n == 0 {
    return 0;
  }
  var q int;
  q = -1;
  var i int;
  for i = range 1@n {
    q = max(q, p[i - 1] + cutRod(n - i));
  }
  return q;
}
